<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_inicio"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.root.appbar.Inicio">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/foch"
        android:id="@+id/Mariscalbtn"
        android:background="@android:color/transparent"
        style="@style/Widget.AppCompat.ImageButton"
        android:adjustViewBounds="false"
        android:layout_below="@+id/textView6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/ronda"
        android:id="@+id/Rondabtn"
        android:background="@null"
        android:layout_marginTop="41dp"
        android:layout_below="@+id/Mariscalbtn"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="@string/ronda_inicio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:textSize="24sp"
        android:fontFamily="serif"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:layout_below="@+id/Mariscalbtn"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="@string/mariscal_inicio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:textStyle="normal|bold"
        android:textSize="24sp"
        android:fontFamily="serif"
        android:textAlignment="center"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
